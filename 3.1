function fetchDataWithPromise(urlAPI) {
    return new Promise((resolve, reject) => {
      const xhttp = new XMLHttpRequest();
      xhttp.open('GET', urlAPI, true);
      xhttp.onreadystatechange = () => {
        if (xhttp.readyState === 4) {
          if (xhttp.status === 200) {
            resolve(JSON.parse(xhttp.responseText));
          } else {
            reject(new Error('No valido' + urlAPI));
          }
        }
      };
      xhttp.send();
    });
  }
  fetchDataWithPromise('https://example.com/data')
  .then(data => {
    console.log(data);
  })
  .catch(error => {
    console.error(error);
  });
  
